<UserControl x:Class="de.as1259.openMPS.UC.Einstellungen.EinstellungenMain"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:de.as1259.openMPS.UC.Einstellungen"
             mc:Ignorable="d"
             d:DesignHeight="600" d:DesignWidth="1050"
             Background="Transparent">
    <Grid>
        <Grid HorizontalAlignment="Left" Height="500" Margin="200,70,0,0" VerticalAlignment="Top" Width="850"
              Background="LightGray" x:Name="grid_setting" />
        <TreeView HorizontalAlignment="Left" Margin="0,70,0,0" Height="500" VerticalAlignment="Top" Width="200"
                  Background="White" x:Name="trv_einstellungen" IsManipulationEnabled="False"
                  SelectedItemChanged="trv_einstellungen_SelectedItemChanged" />

    </Grid>
</UserControl>