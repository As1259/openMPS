<UserControl x:Class="de.fearvel.openMPS.UC.Einstellungen.EinstellungenMainV2"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:de.fearvel.openMPS.UC.Einstellungen"
             mc:Ignorable="d"
             d:DesignHeight="600" d:DesignWidth="1050"
             Background="Transparent" Loaded="UserControl_Loaded">
    <Grid>
        <Grid HorizontalAlignment="Left" Height="500" Margin="200,70,0,0" VerticalAlignment="Top" Width="850"
              Background="LightGray" x:Name="grid_setting" />
        <ListBox HorizontalAlignment="Left" Margin="0,70,0,0" Height="500" VerticalAlignment="Top" Width="200"
                  Background="White" x:Name="ListBoxEinstellungen" IsManipulationEnabled="False" SelectionChanged="ListBoxEinstellungen_SelectionChanged" >
        </ListBox>

    </Grid>
</UserControl>